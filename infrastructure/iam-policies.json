  "CoordinatorAgentPolicy": {
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
          "logs:CreateLogGroup",
          "logs:CreateLogStream",
          "logs:PutLogEvents"
        ],
        "Resource": "arn:aws:logs:*:*:*"
      },
      {
        "Effect": "Allow",
        "Action": [
          "lambda:InvokeFunction"
        ],
        "Resource": [
          "arn:aws:lambda:*:*:function:curriculum-alignment-*"
        ]
      },
      {
        "Effect": "Allow",
        "Action": [
          "states:StartExecution",
          "states:DescribeExecution",
          "states:StopExecution"
        ],
        "Resource": [
          "arn:aws:states:*:*:stateMachine:curriculum-alignment-*"
        ]
      },
      {
        "Effect": "Allow",
        "Action": [
          "events:PutEvents"
        ],
        "Resource": [
          "arn:aws:events:*:*:event-bus/curriculum-alignment-*"
        ]
      },
      {
        "Effect": "Allow",
        "Action": [
          "sqs:SendMessage",
          "sqs:ReceiveMessage",
          "sqs:DeleteMessage",
          "sqs:GetQueueAttributes"
        ],
        "Resource": [
          "arn:aws:sqs:*:*:curriculum-alignment-*"
        ]
      }
    ]
  },
